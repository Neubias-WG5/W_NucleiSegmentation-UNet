{
  "name": "NucleiSegmentation-UNet",
  "description": "",
  "container-image": {
    "image": "neubiaswg5/w_nucleisegmentation-unet",
    "type": "singularity"
  },
  "command-line": "python wrapper.py CYTOMINE_HOST CYTOMINE_PUBLIC_KEY CYTOMINE_PRIVATE_KEY CYTOMINE_ID_PROJECT CYTOMINE_ID_SOFTWARE NUCLEI_MIN_SIZE BOUNDARY_WEIGHT",
  "inputs": [
    {
      "id": "cytomine_host",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine host",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_public_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine public key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_private_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine private key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_id_project",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine project ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "cytomine_id_software",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine software ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "nuclei_min_size",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Minimum estimated size of a nucleus",
      "description": "Minimum estimated size of a nucleus",
      "default-value": 25,
      "set-by-server": false,
      "optional": true,
      "type": "Number"
    },
    {
      "id": "boundary_weight",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Boundary class weight",
      "description": "Boundary class weight in range 0.0-1.0 (larger value results in better object separation but smaller objects)",
      "default-value": 0.0,
      "set-by-server": false,
      "optional": true,
      "type": "Number"
    }
  ],

  "schema-version": "cytomine-0.1"
}
